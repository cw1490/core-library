{
    "name": "webeweb/core-library",
    "type": "library",
    "description": "Core library",
    "keywords": ["webeweb", "library", "core", "algorithm", "argument", "database", "io", "network", "security", "skidata", "sorting", "validation", "curl", "ftp", "sftp"],
    "homepage": "https://github.com/webeweb/core-library",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb/"
        }
    ],
    "require": {
        "ext-curl": "*",
        "ext-intl": "*",
        "ext-zip": "*",
        "php": "^5.6|^7.0",
        "symfony/yaml": "^2.6|^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*|^7.0"
    },
    "support": {
        "issues": "https://github.com/webeweb/core-library/issues"
    },
    "autoload": {
        "psr-4": {
            "WBW\\Library\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Library\\Core\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        }
    }
}
